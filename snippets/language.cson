".source.badger":
  "While":
    "prefix": "wh"
    "body": """
      while ${1:condition} {
        ${2:body}
      }
    """
  "If":
    "prefix": "if"
    "body": """
      if ${1:condition} {
        ${2:body}
      }
    """
  "For In":
    "prefix": "fi"
    "body": """
      for ${1:id} in ${2:value} {
        ${3:body}
      }
    """
